{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start --config craco.config.js",
    "build": "craco build --config craco.config.js",
    "eject": "craco eject",
    "dev": "yarn validate && yarn start",
    "deploy": "yarn global add serve && serve -s build",
    "clean": "rm -rf node_modules",
    "clean-install": "yarn clean && yarn install",
    "analyze": "yarn clean && yarn install && yarn build && source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --no-error-on-unmatched-pattern --quiet --fix --config .eslintrc.js",
    "prettier": "prettier --write \"**/*.+(ts|tsx)\" --config .prettierrc.js",
    "validate": "yarn lint && yarn prettier",
    "test": "jest --logHeapUsage",
    "coverage": "jest --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "bootstrap": "^5.1.3",
    "cors": "^2.8.5",
    "craco": "^0.0.3",
    "express": "^4.18.1",
    "gsap": "^3.10.4",
    "nodemailer": "^6.7.7",
    "react": "^18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-bootstrap-icons": "^1.8.4",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-multi-carousel": "^2.8.2",
    "react-on-screen": "^2.1.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.53.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.25",
    "classnames": "^2.3.1",
    "craco-alias": "^3.0.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  }
}
